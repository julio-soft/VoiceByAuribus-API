{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Formatting.Compact" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.EntityFrameworkCore": "Warning",
        "System.Net.Http.HttpClient": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "formatter": "Serilog.Formatting.Compact.CompactJsonFormatter, Serilog.Formatting.Compact"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId", "WithEnvironmentName" ],
    "Properties": {
      "Application": "VoiceByAuribus-API"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Port=5432;Database=voice_by_auribus_api_db;Username=voice_by_auribus_api_user;Password=voice_by_auribus_api_password"
  },
  "AWS": {
    "Region": "us-east-1",
    "Profile": "default",
    "S3": {
      "AudioFilesBucket": "voice-by-auribus-api",
      "UploadUrlExpirationMinutes": 30,
      "MaxFileSizeMB": 100
    },
    "SQS": {
      "AudioPreprocessingQueueUrl": "https://sqs.us-east-1.amazonaws.com/YOUR_ACCOUNT_ID/audio-preprocessing-queue",
      "AudioPreprocessingQueue": "aurivoice-svs-prep-nbl.fifo",
      "VoiceInferenceQueueUrl": "https://sqs.us-east-1.amazonaws.com/YOUR_ACCOUNT_ID/voice-inference-queue",
      "PreviewInferenceQueueUrl": "https://sqs.us-east-1.amazonaws.com/YOUR_ACCOUNT_ID/preview-inference-queue"
    }
  },
  "Webhooks": {
    "ApiKey": "CHANGE_THIS_IN_PRODUCTION_TO_SECURE_VALUE"
  },
  "Authentication": {
    "Cognito": {
      "Region": "us-east-1",
      "UserPoolId": "us-east-1_2GQIgX9Vw",
      "Audience": "voice-by-auribus-api"
    }
  }
}
